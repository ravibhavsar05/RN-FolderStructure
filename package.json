{
  "name": "react-native-folder-structure",
  "displayName": "React Native Folder Structure Generator",
  "description": "Generate a standard folder structure for React Native projects",
  "icon": "RNLogo.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ravibhavsar05/RN-FolderStructure.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-native-folder-structure.generate",
        "title": "React Native: Generate Folder Structure"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.4"
  }
}